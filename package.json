        "@typescript-eslint/parser": "4.29.2",
        "babel-eslint": "10.1.0",
        "eslint": "7.32.0",
        "eslint-config-standard": "16.0.3",
        "eslint-config-standard-react": "11.0.1",
        "eslint-plugin-import": "2.24.1",
        "eslint-plugin-node": "11.1.0",
